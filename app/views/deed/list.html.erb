<div class="two-columns">
	<div class="inside-column">
                  <%= html_block('right') %>
		<div>
			<h3>Recent Activity</h3>
			<ul>
		    <% for deed in @deeds %>
			     <li><%= render :partial => deed.long_partial, :locals => { :deed => deed } %></li>
		    <% end %>
			</ul>
			<%= link_to('More', { :offset => (@offset.to_i+50), :collection_id => @collection ? @collection.id : nil }) %>
		</div>
  </div>
</div>